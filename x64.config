#
# Automatically generated configuration
# OpenWrt x86_64 optimized for ESXi VM
#

CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_ROOTFS_TARGZ=y
CONFIG_TARGET_ROOTFS_EXT4FS=y
CONFIG_TARGET_EXT4_RESERVED_PCT=0
CONFIG_TARGET_EXT4_BLOCKSIZE_4K=y
CONFIG_TARGET_EXT4_BLOCKSIZE=4096
CONFIG_TARGET_ROOTFS_SQUASHFS=y
CONFIG_TARGET_SQUASHFS_BLOCK_SIZE=256

CONFIG_GRUB_IMAGES=y
CONFIG_GRUB_EFI_IMAGES=y
CONFIG_GRUB_CONSOLE=y
CONFIG_GRUB_BAUDRATE=115200
CONFIG_GRUB_BOOTOPTS=""
CONFIG_GRUB_TIMEOUT="0"
CONFIG_GRUB_TITLE="LEDE"

CONFIG_QCOW2_IMAGES=y
CONFIG_VDI_IMAGES=y
CONFIG_VMDK_IMAGES=y
CONFIG_VHDX_IMAGES=y

CONFIG_TARGET_SERIAL="ttyS0"
CONFIG_TARGET_IMAGES_GZIP=y

CONFIG_TOOLCHAIN_USE_CLANG=y
CONFIG_TOOLCHAIN_USE_LLD=y

CONFIG_KERNEL_DEBUG_INFO=y
CONFIG_KERNEL_DEBUG_INFO_BTF=y
CONFIG_KERNEL_CGROUPS=y
CONFIG_KERNEL_CGROUP_BPF=y
CONFIG_KERNEL_BPF_EVENTS=y
CONFIG_BPF_TOOLCHAIN_HOST=y
CONFIG_KERNEL_XDP_SOCKETS=y

# 压缩工具（按需选择，系统通常已包含基本支持）
CONFIG_PACKAGE_tar=y            # tar 归档工具（必须）
# CONFIG_PACKAGE_bzip2=y          # bzip2 压缩（可选，按需）
CONFIG_PACKAGE_unzip=y          # ZIP 解压（可选，按需）
# CONFIG_PACKAGE_xz-utils=y       # xz 工具集（可选，按需）
# CONFIG_PACKAGE_xz=y             # xz 压缩（可选，按需）

# 文件系统工具（必需）
CONFIG_PACKAGE_e2fsprogs=y      # ext2/3/4 文件系统工具（必须，您使用 ext4）
CONFIG_PACKAGE_mkf2fs=y         # F2FS 文件系统工具（如果使用 F2FS）
CONFIG_PACKAGE_mount-utils=y    # 挂载工具（必须）

# Shell（推荐）
CONFIG_PACKAGE_bash=y

# 核心工具（必需）
CONFIG_PACKAGE_coreutils=y      # 核心 Unix 工具集（必须）
CONFIG_PACKAGE_coreutils-base64=y
CONFIG_PACKAGE_coreutils-nohup=y
CONFIG_PACKAGE_coreutils-stty=y
# LuCI 依赖（必需）
CONFIG_PACKAGE_jq=y             # JSON 处理工具（LuCI 脚本常用）
CONFIG_PACKAGE_jshn=y           # JSON shell 工具（LuCI 脚本必需）
CONFIG_PACKAGE_libjson-script=y # JSON 脚本库（LuCI 必需）
CONFIG_PACKAGE_ucode-mod-html=y # ucode HTML 模块（LuCI 必需）
CONFIG_PACKAGE_ucode-mod-lua=y  # ucode Lua 模块（LuCI 必需）
CONFIG_PACKAGE_openssl-util=y   # OpenSSL 工具（系统常用）

CONFIG_PACKAGE_cfdisk=y         # 磁盘分区工具（图形界面，推荐）
CONFIG_PACKAGE_lsblk=y          # 列出块设备（查看磁盘，推荐）
CONFIG_PACKAGE_partx-utils=y    # 分区工具（推荐）
CONFIG_PACKAGE_hd-idle=y        # 硬盘空闲时休眠（省电，可选）

CONFIG_PACKAGE_bc=y             # 计算器工具（脚本计算用，可选）
CONFIG_PACKAGE_coremark=y       # CPU 性能测试（基准测试，可选）
CONFIG_PACKAGE_pciutils=y      # PCI 设备工具（查看硬件，推荐）
CONFIG_PACKAGE_pciids=y         # PCI ID 数据库（pciutils 依赖）
CONFIG_PACKAGE_sysfsutils=y     # sysfs 工具（系统信息，可选）
CONFIG_PACKAGE_script-utils=y   # 终端录制工具（可选）
CONFIG_PACKAGE_shellsync=y      # Shell 同步工具（可选）
CONFIG_PACKAGE_taskd=y          # 任务守护进程（可选）

CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_frpc=y
CONFIG_PACKAGE_haproxy=y
CONFIG_PACKAGE_ddns-go=y
CONFIG_PACKAGE_uhttpd=y
CONFIG_PACKAGE_microsocks=y
CONFIG_PACKAGE_watchcat=y
CONFIG_PACKAGE_nlbwmon=y
CONFIG_PACKAGE_odhcp6c=y              # IPv6 DHCP 客户端（必须，如果使用 IPv6）
CONFIG_PACKAGE_odhcpd-ipv6only=y      # IPv6 DHCP 服务端（必须，如果使用 IPv6）
CONFIG_PACKAGE_ppp=y                   # PPP 拨号协议（必须，如果使用 PPPoE）
CONFIG_PACKAGE_ppp-mod-pppoe=y         # PPPoE 支持（必须，如果使用 PPPoE）
CONFIG_PACKAGE_vlmcsd=y                # KMS 激活服务（您已有 luci-app-vlmcsd）
CONFIG_PACKAGE_ethtool=y               # 网卡配置工具（调试用，可选）
CONFIG_PACKAGE_tcping=y                # TCP ping 工具（网络测试，可选）
CONFIG_PACKAGE_etherwake=y             # 网络唤醒（如果使用 WOL，可选）
CONFIG_PACKAGE_mwol=y                  # 网络唤醒工具（如果使用 WOL，可选）
CONFIG_PACKAGE_banip=y                 # IP 封禁工具（可选，有防火墙已足够）
# Samba（您已有 luci-app-samba，需要这个后端）
CONFIG_PACKAGE_samba36-server=y

# 主题
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-theme-bootstrap=y

# Base LuCI plugins
CONFIG_PACKAGE_luci-app-zerotier=y
CONFIG_PACKAGE_luci-app-wrtbwmon=y
CONFIG_PACKAGE_luci-app-wol=y
CONFIG_PACKAGE_luci-app-wireguard=y
CONFIG_PACKAGE_luci-app-watchcat=y
CONFIG_PACKAGE_luci-app-arpbind=y
CONFIG_PACKAGE_luci-app-autoreboot=y
CONFIG_PACKAGE_luci-app-filetransfer=y
CONFIG_PACKAGE_luci-app-firewall=y
CONFIG_PACKAGE_luci-app-frpc=y
CONFIG_PACKAGE_luci-app-mwan3=y
CONFIG_PACKAGE_luci-app-mwan3helper=y
CONFIG_PACKAGE_luci-app-nlbwmon=y
CONFIG_PACKAGE_luci-app-ramfree=y
CONFIG_PACKAGE_luci-app-vlmcsd=y
CONFIG_PACKAGE_luci-app-argon-config=y
CONFIG_PACKAGE_luci-app-ddns-go=y
CONFIG_PACKAGE_luci-app-opkg=y
CONFIG_PACKAGE_luci-app-samba=y
CONFIG_PACKAGE_luci-app-store=y
CONFIG_PACKAGE_luci-app-syncdial=y
CONFIG_PACKAGE_luci-app-upnp=y

# openclash
CONFIG_PACKAGE_luci-app-openclash=y
CONFIG_PACKAGE_iptables-mod-tproxy=y
CONFIG_PACKAGE_iptables-mod-extra=y
CONFIG_PACKAGE_ipset=y


# SmartDNS
CONFIG_PACKAGE_smartdns=y
CONFIG_PACKAGE_luci-app-smartdns=y


# turboacc
CONFIG_PACKAGE_luci-app-turboacc=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_FLOW_OFFLOADING=y
# CONFIG_PACKAGE_TURBOACC_INCLUDE_FAST_CLASSIFIER is not set
# CONFIG_PACKAGE_TURBOACC_INCLUDE_SHORTCUT_FE_CM is not set
CONFIG_PACKAGE_TURBOACC_INCLUDE_BBR_CCA=y

# SmartDNS / Rust dependencies
CONFIG_PACKAGE_host-rust=y
CONFIG_PACKAGE_host-rust-bindgen=y

# Network / firewall / TUN
CONFIG_PACKAGE_iptables=y
CONFIG_PACKAGE_iptables-mod-tproxy=y
CONFIG_PACKAGE_iptables-mod-extra=y
CONFIG_PACKAGE_kmod-ipt-nat=y
CONFIG_PACKAGE_ip6tables-mod-nat=y
CONFIG_PACKAGE_kmod-tun=y
CONFIG_PACKAGE_kmod-inet-diag=y
CONFIG_PACKAGE_kmod-nft-tproxy=y

# System optimizations
CONFIG_PACKAGE_net-tools=y
CONFIG_PACKAGE_lm-sensors=y
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_iwinfo=y
CONFIG_PACKAGE_kmod-slhc=y
CONFIG_PACKAGE_kmod-nf-nathelper=y
CONFIG_PACKAGE_kmod-nf-nathelper-extra=y